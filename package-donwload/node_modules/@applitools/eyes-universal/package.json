{
  "name": "@applitools/eyes-universal",
  "version": "2.10.3",
  "description": "Applitools Eyes Universal SDK",
  "keywords": [
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests"
  ],
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/eyes-universal"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "aliases": [
    "universal",
    "usdk"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "bin": {
    "eyes-universal": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "build": "yarn build:dist && yarn build:bin",
    "build:dist": "tsc",
    "build:bin": "pkg .",
    "build:zip": "zip -j ./bin/eyes-universal.zip ./bin/eyes-universal-alpine ./bin/eyes-universal-linux ./bin/eyes-universal-macos ./bin/eyes-universal-win.exe",
    "build:tgz": "tar -czf ./bin/eyes-universal.tar.gz -C ./bin ./eyes-universal-alpine ./eyes-universal-linux ./eyes-universal-macos ./eyes-universal-win.exe",
    "docker:setup": "node ../scripts/scripts/generate-docker-compose-config.js && docker-compose up -d",
    "docker:teardown": "docker-compose down",
    "test": "yarn test:unit && yarn test:it && yarn test:e2e",
    "test:sanity": "yarn test:unit && yarn test:it",
    "test:unit": "mocha ./test/unit/*.spec.ts --exit --no-timeouts -r ts-node/register",
    "test:it": "mocha ./test/it/*.spec.ts --no-timeouts -r ts-node/register",
    "test:e2e": "mocha ./test/e2e/*.spec.ts --exit --no-timeouts -r ts-node/register",
    "deps": "bongo deps",
    "gh:publish": "gh workflow run publish-universal.yml --ref $(git rev-parse --abbrev-ref HEAD)",
    "preversion": "bongo preversion --verify-pending-changes --skip-deps",
    "version": "bongo version --with-pending-changes",
    "postversion": "bongo postversion --skip-release-notification"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn bongo lint"
    }
  },
  "dependencies": {
    "@applitools/execution-grid-client": "1.1.23",
    "@applitools/eyes-sdk-core": "13.8.19",
    "@applitools/logger": "1.1.15",
    "@applitools/utils": "1.3.10",
    "@applitools/visual-grid-client": "15.13.13",
    "proxy-agent": "5.0.0",
    "webdriver": "7.16.11",
    "ws": "7.4.6",
    "yargs": "17.4.1"
  },
  "devDependencies": {
    "@applitools/bongo": "^2.1.6",
    "@applitools/sdk-coverage-tests": "^2.3.20",
    "@applitools/test-server": "^1.1.4",
    "@applitools/test-utils": "^1.4.3",
    "@applitools/types": "1.5.8",
    "@types/http-proxy": "^1.17.9",
    "@types/mocha": "^9.0.0",
    "@types/node": "12",
    "@types/ws": "^7.4.4",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "mocha": "^9.0.0",
    "pkg": "^5.6.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "pkg": {
    "scripts": "./dist/cli.js",
    "outputPath": "./bin",
    "targets": [
      "node14-alpine-x64",
      "node14-linux-x64",
      "node14-macos-x64",
      "node14-win-x64"
    ],
    "compress": "GZip",
    "assets": [
      "./node_modules/vm2/lib/setup-sandbox.js"
    ]
  }
}
