{
  "name": "@applitools/execution-grid-client",
  "version": "1.1.23",
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/execution-grid-client"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "aliases": [
    "eg-client",
    "egc"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./cli": {
      "types": "./types/cli.d.ts",
      "default": "./dist/cli.js"
    },
    "./dist/*": "./dist/*.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "typesVersions": {
    "*": {
      "cli": [
        "./types/cli.d.ts"
      ],
      "dist/*": [
        "./types/*.d.ts"
      ]
    }
  },
  "bin": {
    "eg-client": "./dist/cli.js"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "build": "tsc",
    "test": "mocha --no-timeouts -r ts-node/register ./test/**/*.spec.ts",
    "test:sanity": "yarn test:it && yarn test:e2e",
    "test:it": "mocha --no-timeouts -r ts-node/register ./test/it/*.spec.ts",
    "test:e2e": "mocha --no-timeouts -r ts-node/register ./test/e2e/*.spec.ts",
    "deps": "bongo deps",
    "preversion": "bongo preversion && yarn build",
    "version": "bongo version",
    "postversion": "bongo postversion --skip-release-notification"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn bongo lint"
    }
  },
  "dependencies": {
    "@applitools/logger": "1.1.15",
    "@applitools/utils": "1.3.10",
    "abort-controller": "3.0.0",
    "node-fetch": "2.6.7",
    "proxy-agent": "5.0.0",
    "raw-body": "2.5.1",
    "yargs": "17.4.1"
  },
  "devDependencies": {
    "@applitools/bongo": "^2.1.6",
    "@applitools/eg-tunnel": "^1.0.20",
    "@applitools/scripts": "^1.1.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "12",
    "@types/node-fetch": "^2.6.1",
    "@types/selenium-webdriver": "^4.0.19",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "mocha": "^9.2.2",
    "nock": "^13.2.4",
    "prettier": "^2.6.2",
    "selenium-webdriver": "^4.1.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=12.13.0"
  }
}
